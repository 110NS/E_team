        padding-left: 50px;
        border: solid 2px #000000;
        overflow-x: scroll;
    }

    .f-container {
        width: 1000px;
        display: flex;
        padding: 5px;
    }

    .textarea {
        width: 300px;
        height: 150px;
        margin: 5px;
        padding: 15px;
    }

    #menubar.db {
        align-items: flex-start;
        padding: 100px 0;
    }
</style>
