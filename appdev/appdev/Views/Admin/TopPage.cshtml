                                    <textarea class="textarea" readonly style="width:50%;">ピン止めしたやつを表示する</textarea>
                                    <div class="f-container">
                                        @{
                                            for (int j = 0; j < @Model.list_per_q[i].list.Count; j++)
                                            {

                                                <textarea class="textarea" readonly>
                                                    @Model.list_per_q[i].list[j].student_name
                                                    @Model.list_per_q[i].list[j].text</textarea>
                                            }
                                        }
                                    </div>

                                </div>
                                <br>
                            }
                        }









